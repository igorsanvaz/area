{
    "version": 2,
    "builds": [
      { "src": "api/*.js", "use": "@vercel/node" },
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/calculate", "dest": "/api/calculate.js" },
      { "src": "/api/results", "dest": "/api/results.js" },
      { "src": "/api/users", "dest": "/api/users.js" },
      { "src": "/", "dest": "/index.js" }
    ]
  }
  